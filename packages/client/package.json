{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:rolldown": "vite --config vite.rolldown.config.ts",
    "test": "vitest",
    "test:watch": "vitest --standalone",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "vitest run && playwright test",
    "test:optimizations": "node scripts/validate-optimizations.cjs",
    "coverage": "vitest run --coverage",
    "format": "biome format --write .",
    "lint": "biome check . && oxlint src",
    "build": "tsc -b && vite build",
    "build:rolldown": "tsc -b && vite build --config vite.rolldown.config.ts",
    "preview": "vite preview",
    "formatjs": "formatjs extract"
  },
